"UnlitGeneric"
{
    "$basetexture" "splatoonsweps/effects/flash"
    "$nocull" "1"
    "$nolod" "1"
    "$translucent" "1"
    "$vertexalpha" "1"
    "$vertexcolor" "1"
    "$ignorez" "1"
    "Proxies"
    {
        "AnimatedTexture"
        {
            "animatedTextureVar" "$basetexture"
            "animatedTextureFrameNumVar" "$frame"
            "animatedTextureFrameRate" "5"
        }
    }
}
