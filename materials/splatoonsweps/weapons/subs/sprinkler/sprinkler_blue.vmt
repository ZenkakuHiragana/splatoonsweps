"VertexLitGeneric"
{
    "$basetexture" "splatoonsweps/weapons/subs/sprinkler/sprinkler"
    "$bumpmap" "splatoonsweps/weapons/subs/sprinkler/sprinkler_normal"
    "$lightwarptexture" "splatoonsweps/weapons/primaries/inkling_lightwarp7"

    "$phong" "1"
    "$phongexponent" "50"
    "$phongboost" "12"
    "$phongfresnelranges" "[.3 1 2]"
    "$halflambert" "1"

    "$phongtint" "{ 0 32 66 }"

    "$envmap" "env_cubemap"
    "$envmaptint" "{ 0 32 66 }"
    "$normalmapalphaenvmapmask" "1"

    "$blendtintbybasealpha" "1"
    "$blendtintcoloroverbase" "1"
    "$colortint_base" "{ 248 99 0 }"
    "$colortint_tmp" "[0 0 0]"
    "$color2" "{ 248 99 0 }"

    "proxies"
    {
        "ItemTintColor"
        {
            "resultVar" "$colortint_tmp"
        }
        "SelectFirstIfNonZero"
        {
            "srcVar1" "$colortint_tmp"
            "srcVar2" "$colortint_base"
            "resultVar" "$color2"
        }
    }
}
